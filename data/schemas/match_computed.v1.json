{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://monkeysworks.com/schemas/events/match_computed.v1.json",
  "title": "MatchComputed",
  "type": "object",
  "required": [
    "event_id",
    "event_type",
    "event_version",
    "timestamp",
    "idempotency_key",
    "data"
  ],
  "properties": {
    "event_id": {
      "type": "string",
      "format": "uuid"
    },
    "event_type": {
      "const": "match_computed"
    },
    "event_version": {
      "type": "string",
      "pattern": "^\\d+\\.\\d+$"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "idempotency_key": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "correlation_id": {
      "type": "string",
      "format": "uuid"
    },
    "data": {
      "type": "object",
      "required": [
        "job_id",
        "ranked_freelancers"
      ],
      "properties": {
        "job_id": {
          "type": "string",
          "format": "uuid"
        },
        "model_version": {
          "type": "string"
        },
        "ranked_freelancers": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "freelancer_id": {
                "type": "string",
                "format": "uuid"
              },
              "match_score": {
                "type": "number",
                "minimum": 0,
                "maximum": 1
              },
              "score_breakdown": {
                "type": "object"
              }
            }
          }
        },
        "ab_group": {
          "type": "string",
          "enum": [
            "control",
            "treatment"
          ]
        },
        "latency_ms": {
          "type": "integer"
        }
      }
    }
  }
}